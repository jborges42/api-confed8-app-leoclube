{
    "version": 2,
    "entrypoint": "index.js",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/login",
            "dest": "server.js"
        },
        {
            "src": "/reset-password",
            "dest": "server.js"
        },
        {
            "src": "/register",
            "dest": "server.js"
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}